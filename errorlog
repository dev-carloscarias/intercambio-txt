namespace Walmart.FelGTSoft2000.Mvc.Components
{
    #region Using
    using System;
    using System.Collections.Generic;
    using System.Collections.ObjectModel;
    using System.ComponentModel.DataAnnotations;
    using System.ComponentModel.DataAnnotations.Schema;
    using System.Diagnostics.CodeAnalysis;
    using System.Web;
    #endregion

    [SuppressMessage("StyleCop.CSharp.DocumentationRules", "SA1650:ElementDocumentationMustBeSpelledCorrectly", Justification = "Reviewed.")]

    /// <summary>
    /// Cai view model
    /// </summary>
    [ExcludeFromCodeCoverage]
    [SuppressMessage("StyleCop.CSharp.DocumentationRules", "SA1650:ElementDocumentationMustBeSpelledCorrectly", Justification = "Reviewed.")]
    [SuppressMessage("StyleCop.CSharp.NamingRules", "SA1300:ElementMustBeginWithUpperCaseLetter", Justification = "Reviewed.")]
    public class FolioViewModel
    {
        /// <summary>
        /// Gets or sets Check
        /// </summary>

        [Display(Order =1)]
        public int Check { get; set; }


        [Display(Name = "Idcai", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        public string Idcai { get; set; }

        /// <summary>
        /// Gets or set NumeroCai
        /// </summary>
        [Display(Order = 2, Name = "NumeroCai", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        [Required(ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "NumeroCaiRequired")]
        [RegularExpression(@"^[A-Z0-9-]{1,45}$", ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "OnlyAlphabetNumbers")]
        public string NumeroCai { get; set; }

        ///<summary>
        /// Gets or sets CorrelativoInicial
        /// </summary>
        [Display(Order = 3, Name = "CorrelativoInicial", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        [Required(ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "CorrelativoInicialRequired")]
        [RegularExpression(@"^[0-9-]{1,45}$", ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "OnlyNumbers")]
        public string CorrelativoInicial { get; set; }

        ///<summary>
        /// Gets or sets CorrelativoInicial
        /// </summary>
        [Display(Order = 4, Name = "CorrelativoFinal", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        [Required(ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "CorrelativoFinalRequired")]
        [RegularExpression(@"^[0-9-]{1,45}$", ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "OnlyNumbers")]
        public string CorrelativoFinal { get; set; }

        /// <summary>
        /// Gets or set NumeroCai
        /// </summary>
        [Display(Order = 5, Name = "NumeroSerie", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        [Required(ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "NumeroSerieRequired")]
        [RegularExpression(@"^[A-Z0-9-]{1,45}$", ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "OnlyAlphabetNumbers")]
        public string NumeroSerie { get; set; }

        ///<summary>
        /// Gets or sets CorrelativoInicial
        /// </summary>
        [Display(Order = 6, Name = "LimiteEmision", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        [Required(ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "LimiteEmisionRequired")]
        public string LimiteEmision { get; set; }

        ///<summary>
        /// Gets or sets CorrelativoActual
        /// </summary>
        [Display(Order = 7, Name = "CorrelativoActual", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        public string CorrelativoActual { get; set; }

        ///<summary>
        /// Gets or sets CorrelativoInicial
        /// </summary>
        [Display(Order = 8, Name = "NombreTipoDocumento", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        [Required(ErrorMessageResourceType = typeof(Resources.Components.Folio.Views.CreateFolio), ErrorMessageResourceName = "NombreTipoDocumentoRequired")]
        public string NombreTipoDocumento { get; set; }

        ///<summary>
        /// Gets or sets CorrelativoInicial
        /// </summary>
        [Display(Order = 9, Name = "Estado", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        public string Estado { get; set; }

        /// <summary>
        /// Gets or sets IdPais property
        /// </summary>
        [Display(Name = "IdPais", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        public long IdPais { get; set; }

        /// <summary>
        /// Gets or sets internal model messages
        /// </summary>
        [Display(Name = "Messages", ResourceType = typeof(Resources.Components.Folio.ViewModels.FolioViewModel))]
        public Collection<string> Messages { get; set; }

        /// <summary>
        /// Gets or sets column icon Active
        /// </summary>
        public string Active { get; set; }

        /// <summary>
        /// Gets or sets column icon Inactive
        /// </summary>
        public string Inactive { get; set; }
       
        [NotMapped]

        /// <summary>
        /// Gets or sets Pais property
        /// </summary>
        [Display(Name = "IdPais", ResourceType = typeof(Resources.Components.Vendor.ViewModels.VendorViewModel))]
        public string Pais { get; set; }

    }
}





